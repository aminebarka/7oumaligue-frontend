{
  "name": "7oumaligue-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "prebuild": "rimraf dist && rimraf node_modules/.vite",
    "clean": "rimraf dist && rimraf node_modules/.vite",
    "clean:all": "rimraf dist && rimraf node_modules/.vite && rimraf node_modules && npm install",
    "dev": "vite",
    "build": "tsc && vite build",
    "build:clean": "npm run clean && npm run build",
    "build:azure": "npm run clean && tsc && vite build --config vite.config.azure.ts && node scripts/clean-assets.js",
    "build:minimal": "npm run clean && tsc && vite build --config vite.config.minimal.ts && node scripts/clean-assets.js",
    "build:single": "npm run clean && tsc && vite build --config vite.config.single.ts && node scripts/clean-assets.js",
    "build:ultra": "npm run clean && tsc && vite build --config vite.config.ultra.ts && node scripts/clean-assets.js && node scripts/inline-css.js",
    "build:analyze": "tsc && vite build --mode analyze",
    "clean:assets": "node scripts/clean-assets.js",
    "inline:css": "node scripts/inline-css.js",
    "check:deployment": "node scripts/check-deployment.js",
    "test:build": "node scripts/test-build.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "axios": "^1.6.2",
    "clsx": "^2.0.0",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.61.1",
    "react-hot-toast": "^2.5.2",
    "react-player": "^3.3.1",
    "react-router-dom": "^6.20.1",
    "recharts": "^3.1.0",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.6",
    "tailwind-merge": "^2.1.0",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.3.6",
    "terser": "^5.43.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
